            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/all"
            android:textColor="@color/text_color"
            android:textSize="@dimen/normal"
            android:lineSpacingMultiplier="1.3"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/place"
            android:textColor="@color/text_color"
            android:textSize="@dimen/normal"
            android:lineSpacingMultiplier="1.3"/>
        <TextView
